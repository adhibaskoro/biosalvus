@model Biosalvus.Models.EndageredMapViewModel
@{
    ViewBag.Title = "Threat: Bushfire";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .legend .bar {
        height: 10px;
        width: 100%;
        background: linear-gradient(to right, rgba(255,255,0,1), rgba(255,0,0,1));
        border: 1px solid;
        margin-top: 5px;
    }
    .list {
        cursor: pointer;
        font-size: 12px;
        padding-left: 20px
    }

        .list:hover {
            background-color: lightblue;
        }

    .dropdown-header {
        padding-left: 0px;
    }

    .button {
        border-radius: 4px;
        margin-bottom:5px;
    }

        .button:hover {
            background-color: #4c956c;
            color: white;
        }

    table td {
        border-bottom: 1px solid #000;
        font-size: 12.5px;
    }
</style>
<div style="background-color:#124720;padding-left:15px;padding-right:15px;">
    <header>
        <div style="padding:3rem 5.25rem;background-image:url(../Content/Images/bushfire-banner.jpg);height:auto;width:auto;background-position-x:center;background-repeat:no-repeat;background-size:cover;">
            <div class="line">
                <h1 class="text-white margin-top-bottom-10 text-size-60 text-line-height-1">@ViewBag.Title</h1>
                <p class="margin-bottom-0 text-size-16" style="color:white;">
                    Explore our Dashboard to view the Threatened Birds vs. Bushfire occurances.<br>
                    You can start by selecting the Threatened Status of the birds.<br>
                    Distinct Birds details are selectable from the dropdown.
                </p>
            </div>
        </div>

    </header>
    <br />
    <div class="row">
        <div class="col-md-6 col-xs-12">
            <div style="margin:1px 1px 1px 1px;padding:15px 15px 15px 15px;background-color:white;height:600px;">
                <strong>Map</strong>
                <div class="row" style="margin:1px 1px 1px 1px;">
                    <div class="col-md-6 col-xs-6">
                        <button class="button" id="endangeredbtn" style="padding:3px 3px 3px 3px;font-size:large;width:100%;height:50px;color:white;background-color:#2c6e49;">
                            Endangered
                        </button>
                    </div>
                    <div class="col-md-6 col-xs-6">
                        <button class="button" id="critendangeredbtn" style="padding:3px 3px 3px 3px;font-size:large;width:100%;height:50px;color:white;background-color:#2c6e49;">
                            Critically Endangered
                        </button>
                    </div>
                </div>
                <strong>Note: </strong>Points on map are selectable for distribution details.
                <div id="map" style="height:450px;"></div> <!--This is the Map-->
                <div class="row" style="margin-top:10px;margin-bottom:10px;">
                    <div class="col-md-6 col-xs-6">Frequency of Bushfire:</div>
                    <div class="col-md-6 col-xs-6">
                        <div id="legend" class="legend">
                            <div class="bar"></div>
                            <div class="row">
                                <div class="col-md-6 col-xs-6" style="text-align:left;">LOW</div><div class="col-md-6 col-xs-6" style="text-align:right;">HIGH</div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--COL 2-->
        <div class="col-md-3 col-xs-12">
            <!--DROPDOWN-->
            <div style="margin:1px 1px 1px 1px;padding:15px 15px 15px 15px;background-color:white;height:600px;">
                <strong>Bird Details</strong>
                <div class="dropdown">
                    <button class="btn btn-default dropdown-toggle" type="button" data-toggle="dropdown" style="padding:3px 3px 3px 3px;width:100%;height:50px;text-align:center;font-size:large;">
                        <span id="dropdowntitle">Australasian Bittern</span>
                        <span class="caret"></span>
                    </button>
                    <ul class="dropdown-menu">
                        <li id="endangeredheader" class="dropdown-header"><strong>Endangered</strong></li>
                        <li id="australasianbitternlst" class="list">Australasian Bittern</li>
                        <li id="azurekingfisherlst" class="list">Azure Kingfisher</li>
                        <li id="blackearedminerlst" class="list">Black-eared Miner</li>
                        <li id="brownthornbilllst" class="list">Brown Thornbill</li>
                        <li id="easternbristlebirdlst" class="list">Eastern Bristlebird</li>
                        <li id="easterngroundparrotlst" class="list">Eastern Ground Parrot</li>
                        <li id="glossyblackcockatoolst" class="list">Glossy Black-cockatoo</li>
                        @*<li id="helmetedhoneyeaterlst" class="list" style="display:none;">Helmeted Honeyeater</li>
                            <li id="hoodedrobinlst" class="list" style="display:none;">Hooded Robin</li>
                            <li id="huahoulst" class="list" style="display:none;">Huahou</li>
                            <li id="malleeemuwrenlst" class="list" style="display:none;">Mallee Emu-wren</li>
                            <li id="redtailedblackcockatoolst" class="list" style="display:none;">Red-tailed Black Cockatoo</li>
                            <li id="southernemuwrenlst" class="list" style="display:none;">Southern Emu-wren</li>
                            <li id="southerngiantpetrellst" class="list" style="display:none;">southern giant-petrel</li>
                            <li id="swiftparrotlst" class="list" style="display:none;">Swift Parrot</li>
                            <li id="wedgetailedeaglelst" class="list" style="display:none;">Wedge-tailed Eagle</li>
                            <li id="yellowtuftedhoneyeaterlst" class="list" style="display:none;">Yellow-tufted Honeyeater</li>*@
                        <li id="critendangeredheader" class="dropdown-header"><strong>Critically Endangered.</strong></li>
                        <li id="curlewsandpiperlst" class="list">Curlew Sandpiper</li>
                        <li id="easterncurlewlst" class="list">Eastern curlew</li>
                        @*<li id="greatknotlst" class="list" style="display:none;">Great Knot</li>
                            <li id="orangebelliedparrotlst" class="list" style="display:none;">Orange-bellied Parrot</li>
                            <li id="plainswandererlst" class="list" style="display:none;">Plains-wanderer</li>
                            <li id="regenthoneyeaterlst" class="list" style="display:none;">Regent Honeyeater</li>
                            <li id="spottedquailthrushlst" class="list" style="display:none;">Spotted Quail-thrush</li>*@
                    </ul>
                </div>
                <!-- Australasian Bittern -->
                <div class="birddetail" id="australasianbittern" style="display:block;">
                    <div class="row">
                        <div class="col-md-12 col-xs-12">
                            <img class="img-responsive" style="margin-left:auto;margin-right:auto;height:100px;" src="~/Content/Images/Birds/australasian-bittern.jpg" alt="australasianbittern" />
                        </div>
                        <div class="col-md-12 col-xs-12">
                            <p>The Australasian bittern, also known as the brown bittern or matuku hūrepo, is a large bird in the heron family Ardeidae. A secretive bird with a distinctive booming call, it is more often heard than seen. Australasian bitterns are endangered in both Australia and New Zealand.</p>
                        </div>
                        <div class="col-md-12 col-xs-12">
                            <table class="table">
                                <tr><th data-toggle="tooltip" title="Total historal count from data source">Spotting</th><th data-toggle="tooltip" title="Registered PSMA Administrative Boundaries">City/Council</th><th data-toggle="tooltip" title="Frequency. H: >75%, M: >50%, L: >0%">Fire Level</th></tr>
                                @foreach (var item in Model.australasionBitternRankings.Take(5))
                                {
                                    <tr class="birddata">
                                        <td class="birdcount">@Html.DisplayFor(modelitem => item.birdcount)</td>

                                        <td class="birdCity/Council">@Html.DisplayFor(modelitem => item.locality)</td>
                                        <td class="threatrate">@Html.DisplayFor(modelitem => item.threatrate)</td>
                                    </tr>
                                }
                            </table>
                        </div>
                    </div>
                </div>
                <!-- Azure Kingfisher -->
                <div class="birddetail" id="azurekingfisher" style="display:none;">
                    <div class="row">
                        <div class="col-md-12 col-xs-12">
                            <img class="img-responsive" style="margin-left:auto;margin-right:auto;height:100px;" src="~/Content/Images/Birds/azure-kingfisher.jpg" alt="azurekingfisher" />
                        </div>
                        <div class="col-md-12 col-xs-12">
                            <p>The azure kingfisher is a small kingfisher, in the river kingfisher subfamily, Alcedininae. It is found in Northern and Eastern Australia and Tasmania, as well as the lowlands of New Guinea and neighbouring islands, and out to North Maluku and Romang.</p>
                        </div>
                        <div class="col-md-12 col-xs-12">
                            <table class="table">
                                <tr><th data-toggle="tooltip" title="Total historal count from data source">Spotting</th><th data-toggle="tooltip" title="Registered PSMA Administrative Boundaries">City/Council</th><th data-toggle="tooltip" title="Frequency. H: >75%, M: >50%, L: >0%">Fire Level</th></tr>
